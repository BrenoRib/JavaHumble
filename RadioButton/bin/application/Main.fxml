<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="application.MainController">
   <children>
      <RadioButton fx:id="rb1" layoutX="45.0" layoutY="64.0" mnemonicParsing="false" onAction="#radioSelect" text="Male">
         <toggleGroup>
            <ToggleGroup fx:id="Mygroup" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="rb2" layoutX="45.0" layoutY="123.0" mnemonicParsing="false" onAction="#radioSelect" text="Female" toggleGroup="$Mygroup" />
      <Label fx:id="lbl" layoutX="75.0" layoutY="189.0" prefHeight="17.0" prefWidth="132.0" />
   </children>
</AnchorPane>
